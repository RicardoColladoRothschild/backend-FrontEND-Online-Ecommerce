{
    "rewrites":[
        {
            "source": "/",
            "destination": "/api"
          },
        {
            "source":"/api/(.*)",
            "destination":"/api"
        },
        {
            "source": "/(.*)",
            "destination": "/$1"
        }
    ],
    "builds":[
        {
            "src": "./api/index.js",
            "use": "@vercel/node"
        }
    ]
}