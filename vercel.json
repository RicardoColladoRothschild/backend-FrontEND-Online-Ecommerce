{
    "rewrites":[
        {
            "source":"/api/(.*)",
            "destination":"/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": ".index.js"
        }
    ],
    "builds":[
        {
            "src": "./api/index.js",
            "use": "@vercel/node"
        }
    ]
}
